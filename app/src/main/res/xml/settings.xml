<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:attrs="http://schemas.android.com/apk/res/me.podrezov.cp481">
    <me.podrezov.cp481.SeekBarPreference
        android:key="objectSpeed"
        android:title="@string/speed"
        android:dialogTitle="@string/speed"
        android:summary="@string/speed_summary"
        android:persistent="true"
        android:defaultValue="20"
        attrs:minValue="10"
        attrs:maxValue="50" />

    <com.rarepebble.colorpicker.ColorPreference
        android:key="colorObject"
        android:title="@string/color_object"
        android:defaultValue="#f00"
        />

    <com.rarepebble.colorpicker.ColorPreference
        android:key="colorBackground"
        android:title="@string/color_background"
        android:defaultValue="#fff"
        />
</PreferenceScreen>